{
  "name": "postgis-wkt-test-service",
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "preinstall": "npm link postgraphile-postgis-wkt || echo 'Linked package not available - install manually if developing locally'",
    "postinstall": "npm run build",
    "link-package": "npm link ../path/to/postgraphile-postgis-wkt",
    "unlink-package": "npm unlink postgraphile-postgis-wkt",
    "build": "tsc",
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "old:start": "GRAPHILE_ENV=development node --loader=ts-node/esm src/server.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^4.21.2",
    "graphql-type-json": "^0.3.2",
    "pg": "^8.14.1",
    "postgis": "^1.0.5",
    "postgraphile": "^5.0.0-beta.40"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.30",
    "eslint": "^9.20.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1"
  },
  "overrides": {
    "eslint-plugin-graphile-export": {
      "eslint": "^9.20.1"
    }
  }
}
